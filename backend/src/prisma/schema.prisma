generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id          String  @id @default(uuid())
  status      Status
  price       Float
  leverage    Int
  marginValue Float
  couple      String
  isOpen      Boolean
}

enum Status {
  long
  short
}
